			<Parameter name="get_gains" refId="id_real_time_get_reference_gain2"/>
			<Parameter name="key" value="IE_CH3_REF_GAIN"/>
		  </Associator>
		</ConfigItem>
		<ConfigItem class="ucTextBox" present="hex" Unique="DSP_PARA_INEAR_STRU.IE_CH4_REF_GAIN">
		  <Associator class="Airoha.Project.ConfigTool.Associator.fcAecReferenceGainAssociator">
			<Parameter name="init_gains" refId="id_real_time_init_reference_gain2"/>
			<Parameter name="get_gains" refId="id_real_time_get_reference_gain2"/>
			<Parameter name="key" value="IE_CH4_REF_GAIN"/>
		  </Associator>
		</ConfigItem>
	  </ConfigGroup>
	</ConfigGroup>
