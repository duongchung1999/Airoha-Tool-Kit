	<ConfigItem class="ucTextBox" present="hex" Unique="DSP_PARA_INEAR_STRU.IE_FB_MIC_ATTEN" visible="hidden"/>
	<ConfigItem class="ucTextBox" present="hex" Unique="DSP_PARA_INEAR_STRU.IE_MID_WIND_ATTEN" visible="hidden"/>
	<ConfigItem class="ucTextBox" present="hex" Unique="DSP_PARA_INEAR_STRU.IE_SBEC_PF_order_12" visible="hidden"/>
	<ConfigItem class="ucTextBox" present="hex" Unique="DSP_PARA_INEAR_STRU.IE_SBEC_PF_order_34" visible="hidden"/>
