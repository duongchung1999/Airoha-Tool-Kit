            <Parameter name="unique" value="DSP_PARA_SWB_RX_VO_CPD_STRU.out_max"/>
          </ConfigItem>
          <ConfigItem class="ucComboBox" id="id_rx_swb_cpd_gain_cb">
            <Parameter name="top_label" value="CPD Positive Gain:"/>
            <Parameter name="bottom_label" value="(0 ~ 16 db)"/>
